{
  "version": 2,
  "builds": [
    {
      "src": "bloodbankmanager/wgsi.py"
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
    }
}
],
"routes": [
  {
    "src": "/static/(.*)",
    "dest": "bloodbankmanager/wsgi.py"
  }
  ]
}
